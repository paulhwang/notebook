alias mkdb="cds; mkdir target_dir; mkdir db_dir"
alias gitclonesql="cdg; git clone https://github.com/paulhwang/sql_go; mkdb"
alias ipaddr="ip address"
alias ipaddrext="curl -w "\n" -s https://api.ipify.org"
