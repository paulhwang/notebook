alias a='alias'
a h='history'
a l='ls'
a ll='ls -l'
a lll='ls -a'
a llll='ls -al'
a key='cat ~/mine/git_dir/key'
a cdm='cd; cd mine'
a cdw='cdm; cd work'
a cdc='cdw; cd c_go'
a cdn='cdw; cd n_go'
a cdj='cdw; cd j_go'
a cdnotebook='cdw; cd notebook'
a cdpython='cdm; cd python_dir'
alias cdpythonenvironments="cdpython; cd environments; source my_env/bin/activate"
a au='cd ~; vi .alias_phwang; au_copy; au_go'
a au_copy='cp ~/.alias_phwang ~/mine/work/notebook/linux_dir/alias_phwang'
a au_go='source ~/.alias_phwang'

alias lld="cds; ll db_dir"
alias llt="cds; ll target_dir"
alias go1="cds; target_dir/sql_server"
alias cdg="cd; cd github"

alias cds="cdg; cd sql_go"
alias goc="cdc; ./server"
alias gon="cdn; ./run_go"
alias cdss="cds; cd sql_server_dir"
alias cdt="cds; cd test_dir"
alias cdu="cds; cd utils_dir"
